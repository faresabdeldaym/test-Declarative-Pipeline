pipeline {
    agent any

    stages {
        stage('SCM checkout') {
            steps {
                echo 'Getting From Github'
                echo 'SCM checkout Completed'
            }
        }
        stage('Build') {
            steps {
                echo 'Build Completed'
            }
        }
        stage('Test') {
            steps {
                echo 'Test Completed'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy Completed'
            }
        }
    }
}
